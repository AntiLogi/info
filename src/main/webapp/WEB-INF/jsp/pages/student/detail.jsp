              <strong><i class="fa fa-map-marker margin-r-5"></i> Location</strong>

              <p class="text-muted">
                ${student.address}
              </p>

              <hr>

              <strong><i class="fa fa-pencil margin-r-5"></i> Languege Skills</strong>

              <p>
                <span class="label label-danger">UI Design</span>
                <span class="label label-success">Coding</span>
                <span class="label label-info">Javascript</span>
                <span class="label label-warning">PHP</span>
                <span class="label label-primary">Node.js</span>
              </p>

              <hr>

              <strong><i class="fa fa-file-text-o margin-r-5"></i> 家庭信息</strong>

              <p>
                父亲：${student.fatherName}   生日：<fmt:formatDate value="${student.fatherBirthday}" pattern="yyyy-MM-dd"/> <br/>
                母亲：  ${student.motherName}  生日：<fmt:formatDate value="${student.motherBirthday}" pattern="yyyy-MM-dd"/> <br/>
                费用来源：${student.costSource}

              </p>

            <hr/>
              <strong><i class="fa fa-book margin-r-5"></i> 联系方式</strong>
              <p class="text-muted">
                电话：${student.telephone}  <br/>
                手机：${student.mobile}  <br/>
                qq：${student.qq}<br/>
                微信：${student.wechat}<br/>
                邮箱：${student.email}<br/>
                紧急联系人：${student.emergencyContact}<br/>
                紧急联系电话：${student.emergencyContactPhone}<br/>
                身份证号码：${student.identityCard}<br/>

